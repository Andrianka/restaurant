%fieldset
  .form-body
    .form-group.row
      .control-label.col-md-3= f.label :email
      .col-md-9
        = f.email_field :email, autofocus: true, :class => 'form-control'
    .form-group.row
      .control-label.col-md-3= f.label :password
      .col-md-9
        = f.password_field :password, autocomplete: "off", :class => 'form-control'
    .form-group.row
      .control-label.col-md-3= f.label :password_confirmation
      .col-md-9
        = f.password_field :password_confirmation, autocomplete: "off", :class => 'form-control'
